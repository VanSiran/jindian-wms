<odoo>
    <template id="menu_secondary" inherit_id="web.menu_secondary">
        <xpath expr='//div[@class="o_sub_menu_footer"]' position='replace'>
          <div class="o_sub_menu_footer">
              维护管理员电话 <span style="color:#875a7b;">13315821782</span><br/>
              技术支持 电话  <span style="color:#875a7b;">13315821782</span>
          </div>
        </xpath>
    </template>
    <template id="layout" inherit_id="web.layout">
        <xpath expr='//title' position='replace'>
             <title t-esc="title or '天津电务段仓库管理系统'"/>
        </xpath>
    </template>

    <template id="login" inherit_id="web.login">
      <xpath expr='//label[@for="login"]' position="replace">
        <label for="login" class="control-label">手机</label>
      </xpath>
    </template>
</odoo>

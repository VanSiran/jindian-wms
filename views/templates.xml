<odoo>
    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr='//script[@src="/web/static/src/js/chrome/action_manager.js"]' position='replace'>
            <script type="text/javascript" src="/wms/static/src/js/action_manager.js"></script>
        </xpath>
        <xpath expr='//script[@src="/web/static/src/js/views/list/list_renderer.js"]' position='replace'>
            <script type="text/javascript" src="/wms/static/src/js/list_renderer.js"></script>
        </xpath>
        <xpath expr='//script[@src="/web/static/src/js/chrome/control_panel.js"]' position='replace'>
            <script type="text/javascript" src="/wms/static/src/js/control_panel.js"></script>
        </xpath>
        <xpath expr='//script[@src="/web/static/src/js/chrome/abstract_web_client.js"]' position='replace'>
            <script type="text/javascript" src="/wms/static/src/js/abstract_web_client.js"></script>
        </xpath>
        <xpath expr='//script[@src="/web/static/src/js/views/basic/basic_model.js"]' position='replace'>
            <script type="text/javascript" src="/wms/static/src/js/basic_model.js"></script>
        </xpath>
        <!-- <xpath expr='//script[@src="/web/static/src/js/fields/basic_fields.js"]' position='replace'>
            <script type="text/javascript" src="/wms/static/src/js/basic_fields.js"></script>
        </xpath> -->

        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css"
                href="/wms/static/src/css/style.min.css"/>
            <link rel="stylesheet" type="text/css"
                href="/wms/static/src/css/zoomify.min.css"/>

            <script type="text/javascript"
                src="/wms/static/src/js/enter_to_submit.js" />
            <!-- <script type="text/javascript"
                src="/wms/static/src/js/showwarn_field.js" /> -->
            <!-- <script type="text/javascript"
                src="/wms/static/src/js/filter_search.js" /> -->
            <script type="text/javascript"
                src="/wms/static/src/js/jstree.min.js" />
            <script type="text/javascript"
                src="/wms/static/src/js/tree_selection.js" />
            <script type="text/javascript"
                src="/wms/static/src/js/zoomify.min.js" />
            <script type="text/javascript"
                src="/wms/static/src/js/zoomable_image.js" />
        </xpath>
    </template>
</odoo>
